{
  "extends": [
    "xo-space/browser",
    "xo-space/esnext",
    "xo-react",
    "prettier",
    "prettier/react",
    "plugin:jest/recommended"
  ],
  "rules": {
    "react/prop-types": 0,
    "new-cap": 0,
    "jsx-a11y/alt-text": 2
  },
  "env": {
    "jest": true
  },
  "plugins": [
    "jsx-a11y"
  ]
}
